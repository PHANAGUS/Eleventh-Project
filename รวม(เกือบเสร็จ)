        cout << "Find Sy max" << "\n";
        func2findsymax();
        }
    else if(y==5){
    cout << "Find Sx max" << "\n";
    func3findsxmax();
        }
    else if(y==6){ //find t
        int x6;
        cout << "Know radius              press 1" << "\n";
        cout << "Don't know radius        press 2" << "\n";
