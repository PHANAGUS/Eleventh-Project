void hav2(){
    cout << "-------------------------" << endl;
    cout << "Vx = w*sqrt(R^2 - x^2)" << endl;
    cout << "w = "; cin >> w;
    cout << "R = "; cin >> R;
    cout << "x = "; cin >> x;
    cout << "Vx = " << w*sqrt(pow(R,2)-pow(x,2)) << " m/s" << endl;
}
void hav3(){
    cout << "-------------------------" << endl;
    cout << "Vt = wRsin(wt)" << endl;
    cout << "w = "; cin >> w;
    cout << "R = "; cin >> R;
    cout << "t = "; cin >> t;
    cout << "Vt = " << w*R*sin(w*t) << " m/s" << endl;
}
