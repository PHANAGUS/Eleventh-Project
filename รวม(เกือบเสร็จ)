void penf(){
    cout << "-------------------------" << endl;
    cout << "1/f = 2(PI) * sqrt(g/l)"<< endl;
    cout << "l = "; cin >> l;
    cout << "g = "; cin >> g;
    cout << "f = " << 1/2*(M_PI)*sqrt(g/l) << " Hz";
}
