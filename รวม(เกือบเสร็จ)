    cout << "-------------------------" << endl;
    cout << "w = sqrt(k,m)" << endl;
    cout << "k = "; cin >> k;
    cout << "w = "; cin >> w;
    cout << "m = "<< k/pow(w,2) << " kg"<< endl;
}
void spm3(){
    cout << "-------------------------" << endl;
    cout << "T = 2(PI) * sqrt(k,m)" << endl;
    cout << "k = "; cin >> k;
    cout << "T = "; cin >> T;
    cout << "m = "<< 2*(M_PI)*k/pow(T,2) << " kg"<< endl;
}
void spm4(){
    cout << "-------------------------" << endl;
    cout << "f = 1/2(PI) * sqrt(k,m)" << endl;
    cout << "k = "; cin >> k;
    cout << "f = "; cin >> f;
    cout << "m = "<< k/pow(2*(M_PI)*f,2) << " kg"<< endl;
}
//findm1 เทียบ2ตัวแปร
void spm51(){
    cout << "-------------------------" << endl;
    cout << "T2/T1 = sqrt(m2/m1)" << endl;
    cout << "T1 = "; cin >> T1;
    cout << "T2 = "; cin >> T2;
    cout << "m2 = "; cin >> m2;
    cout << "m1 = "<< m2*pow(T1,2) / pow(T2,2) << " kg"<< endl;
}
//findm2 เทียบ2ตัวแปร
void spm52(){
    cout << "-------------------------" << endl;
    cout << "T2/T1 = sqrt(m2/m1)" << endl;
    cout << "T1 = "; cin >> T1;
    cout << "T2 = "; cin >> T2;
    cout << "m1 = "; cin >> m2;
    cout << "m2 = "<< m1*pow((T2/T1),2) << " kg"<< endl;
}
