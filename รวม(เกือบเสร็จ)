{
    double ans;
    if(tofind == "Ek")
    {
        cout << "I = "; cin >> I;
        cout << "w = "; cin >> w;
        Ek = (1/2)*I*w*w;
        ans = Ek;
    }
    return ans;
}*/
double Chapter5_6(string tofind)
{
    double ans;
    if(tofind == "Ep")
    {
        cout << "m = "; cin >> m;
        cout << "h = "; cin >> h;
        Ep = m*g*h;
        ans = Ep;
    }
    return ans;
}
double Chapter5_7(string tofind)
{
    double ans;
    if(tofind == "Ep")
    {
        cout << "k = "; cin >> k;
        cout << "x = "; cin >> x;
        Ep = (1/2)*k*x*x;
        ans = Ep;
    }
    else if(tofind == "k")
    {
        cout << "Ep = "; cin >> Ep;
        cout << "x = "; cin >> x;
        k = (2*Ep)/(x*x);
        ans = k;
    }
    else if(tofind == "x")
    {
        cout << "Ep = "; cin >> Ep;
        cout << "k = "; cin >> k;
        x = pow(2*Ep/k,1/2);
        ans = x;
    }
    return ans;
}
double Chapter5_8(string tofind)
{
    double ans;
    if(tofind == "k")
    {
        cout << "F = "; cin >> F;
        cout << "x = "; cin >> x;
        k = F/x;
        ans = k;
    }
    else if(tofind == "F")
    {
        cout << "k = "; cin >> k;
        cout << "x = "; cin >> x;
        F = k*x;
        ans = F;
    }
    else if(tofind == "x")
    {
        cout << "F = "; cin >> F;
        cout << "k = "; cin >> k;
        x = F/k;
        ans = x;
    }
    return ans;
}

void findW()
{
    string select;
    cout << "--------------------------------------------------" << endl;
    cout << "Find by" << endl;
    cout << "   1. F(Force) and s(Displacement)         Press 1" << endl;
    cout << "   2. P(Power) and T(Time)                 Press 2" << endl;
    do{
        cout << "Press : ";
        getline(cin,select);
        if(select == "1" || select == "2") break;
        else cout << "***Please Press 1-2." << endl;
    }while(true);
    cout << "--------------------------------------------------" << endl;

    if(select == "1") Chapter5_1("W");
    else if(select == "2") Chapter5_2("W");

    cout << "#Result ----->  W = " << W  << " N.m" << endl;
    cout << "--------------------------------------------------";
}
