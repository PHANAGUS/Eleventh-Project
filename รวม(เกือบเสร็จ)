        else if (findu2 == 4){ func24findu();}  
        else if (findu2 == 5){ func25findu();}   
        else if (findu2 == 3){ func26findu();}
        else {cout << "Can not solve ur number";}
        }
        else {cout << "Can not solve ur number";}
    }
